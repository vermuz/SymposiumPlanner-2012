% ------------------------------------------------------------------------------------------------------
% Basic ContractLog Libaries
% 
% ContractLog includes all basic libaries. Use this instead of manual include.
% Problems may arise, if some libaries included double time!
% ------------------------------------------------------------------------------------------------------

moduleOID("./ContractLog/ContractLog.prova").

:-eval(consult('./ContractLog/math.prova')). % math library
:-eval(consult('./ContractLog/list.prova')). % list library
:-eval(consult('./ContractLog/list_math.prova')). % list math library
:-eval(consult('./ContractLog/datetime.prova')). % date/time library
:-eval(consult('./ContractLog/timeinterval.prova')). % time interval library
:-eval(consult('./ContractLog/ec.prova')). % event calculus library
:-eval(consult('./ContractLog/deontic.prova')). % deontic logic library
:-eval(consult('./ContractLog/defeasible_deontic.prova')). % defeasible deontic logic library
:-eval(consult('./ContractLog/utils.prova')). % utils
:-eval(consult('./ContractLog/owl.prova')). % owl library
:-eval(consult('./ContractLog/ecaruntime.prova')). %eca runtime
:-eval(consult('./ContractLog/cache.prova')). %eca runtime
:-eval(consult('./ContractLog/update.prova')). %eca runtime
 
